## bash script to automate testing L1 Data cache performance across 4 associativity levels

make clean
make all

## testing for lib_quantum benchmark
pin -t obj-intel64/main.so -config cA-128 -outfile tA-128.txt -max_inst 100000 -- /home/cs246/benchmarks/libquantum_O3 400 25
pin -t obj-intel64/main.so -config cA-256 -outfile tA-256.txt -max_inst 100000 -- /home/cs246/benchmarks/libquantum_O3 400 25
pin -t obj-intel64/main.so -config cA-512 -outfile tA-512.txt -max_inst 100000 -- /home/cs246/benchmarks/libquantum_O3 400 25
pin -t obj-intel64/main.so -config cA-1024 -outfile tA-1024.txt -max_inst 100000 -- /home/cs246/benchmarks/libquantum_O3 400 25
